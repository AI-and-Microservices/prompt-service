{
  "name": "prompt-service",
  "version": "1.0.0",
  "description": "Prompt microservice with Express, MongoDB, Kafka, JWT, and Trace Logging",
  "main": "server.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "dev": "nodemon server --x-service=prompt",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "start": "node server --x-service=prompt",
    "up": "bash bin/up.sh"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": "prettier --write",
    "package.json": "npx -y sort-package-json"
  },
  "author": "khangpv",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-list-endpoints": "^7.1.1",
    "google-auth-library": "^9.15.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongodb-memory-server": "^10.1.4",
    "mongoose": "^8.14.0",
    "redis": "^5.0.0",
    "supertest": "^7.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
